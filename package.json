{
  "name": "lego_lister",
  "productName": "Lego Lister",
  "version": "1.0.0",
  "description": "Streamlines the process of listing a Lego set on eBay.",
  "repository": "https://github.com/jwrand16/Lego-Lister",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-mac": "electron-packager . --platform=darwin --arch=x64 --icon=icons/mac/icon.icns --out=release-builds --overwrite ",
    "build-win": "electron-packager . --platform=win32 --arch=x64 --icon=icons/win/icon.ico --out=release-builds --overwrite "
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "dropbox": "^8.2.0"
  },
  "author": "James Randolph",
  "license": "ISC",
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-builder": "^22.6.1",
    "electron-packager": "^15.2.0"
  }
}
